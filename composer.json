{
  "name": "tallesttree/topicalboost-drupal",
  "description": "AI-powered topic analysis and structured data generation for Drupal. Automatically analyzes content to identify relevant topics and generates schema.org markup for improved SEO.",
  "type": "drupal-module",
  "extra": {
    "installer-name": "ttd_topics"
  },
  "license": "GPL-2.0-or-later",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "drupal",
    "seo",
    "topics",
    "schema.org",
    "ai",
    "content-analysis",
    "structured-data",
    "topical-authority"
  ],
  "homepage": "https://topicalboost.com",
  "support": {
    "issues": "https://github.com/TallestTreeDigital/topicalboost-drupal/issues",
    "source": "https://github.com/TallestTreeDigital/topicalboost-drupal"
  },
  "authors": [
    {
      "name": "TallestTree Digital",
      "homepage": "https://tallesttree.digital",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "drupal/core": "^9.5 || ^10.0 || ^11.0",
    "drupal/advancedqueue": "^1.0",
    "drupal/pathauto": "^1.0"
  },
  "require-dev": {
    "drupal/core-dev": "^9.5 || ^10.0 || ^11.0"
  },
  "autoload": {
    "psr-4": {
      "Drupal\\ttd_topics\\": "src/"
    }
  }
}
